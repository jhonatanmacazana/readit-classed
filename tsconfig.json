{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    //  "forceConsistentCasingInFileNames": true,
    "lib": ["es5", "es6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "paths": {
      "#root/*": ["./src/*"]
    },
    "skipLibCheck": true,
    //  "strict": true,
    //  "strictPropertyInitialization": true,
    "sourceMap": true,
    "target": "es5"
  }
}
